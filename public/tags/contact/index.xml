    <item>
      <title>Contact</title>
      <link>http://www.yearzero.me.uk/contact/</link>
      <pubDate>Sun, 01 Jan 2017 15:25:29 +0000</pubDate>
      
      <guid>http://www.yearzero.me.uk/contact/</guid>
      <description>&lt;div class=&#34;col-md-8&#34; style=&#34;padding-top:20px;&#34;&gt;
      &lt;form class=&#34;form-horizontal&#34; role=&#34;form&#34; action=&#34;https://formspree.io/hello@yearzero.space&#34; method=&#34;POST&#34;&gt;
        &lt;div class=&#34;form-group&#34;&gt;
          &lt;label for=&#34;name&#34; class=&#34;col-sm-2 control-label&#34;&gt;Name&lt;/label&gt;
          &lt;div class=&#34;col-sm-10&#34;&gt;
            &lt;input type=&#34;text&#34; class=&#34;form-control&#34; id=&#34;name&#34; name=&#34;name&#34; placeholder=&#34;First &amp; Last Name&#34; value=&#34;&#34;&gt;
          &lt;/div&gt;
        &lt;/div&gt;
        &lt;div class=&#34;form-group&#34;&gt;
          &lt;label for=&#34;email&#34; class=&#34;col-sm-2 control-label&#34;&gt;Email&lt;/label&gt;
          &lt;div class=&#34;col-sm-10&#34;&gt;
            &lt;input type=&#34;email&#34; class=&#34;form-control&#34; id=&#34;email&#34; name=&#34;email&#34; placeholder=&#34;example@domain.com&#34; value=&#34;&#34;&gt;
          &lt;/div&gt;
        &lt;/div&gt;
        &lt;div class=&#34;form-group&#34;&gt;
          &lt;label for=&#34;message&#34; class=&#34;col-sm-2 control-label&#34;&gt;Message&lt;/label&gt;
          &lt;div class=&#34;col-sm-10&#34;&gt;
            &lt;textarea class=&#34;form-control&#34; rows=&#34;4&#34; name=&#34;message&#34;&gt;&lt;/textarea&gt;
          &lt;/div&gt;
        &lt;/div&gt;
        &lt;div class=&#34;form-group&#34;&gt;
          &lt;div class=&#34;col-sm-10 col-sm-offset-2&#34;&gt;
            &lt;input id=&#34;submit&#34; name=&#34;submit&#34; type=&#34;submit&#34; value=&#34;Send&#34; class=&#34;btn btn-primary&#34;&gt;
          &lt;/div&gt;
        &lt;/div&gt;
      &lt;/form&gt;
    &lt;/div&gt;
</description>
